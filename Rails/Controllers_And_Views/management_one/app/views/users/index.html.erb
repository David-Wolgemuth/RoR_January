<h1>Users</h1>

<table>
  <tr>
    <th>First_name</th>
    <th>Last_name</th>
    <th>Email</th>

  </tr>
  <% @users.each do |user|%>

  <tr>
<td><%= user.first_name %></td>
<td><%= user.last_name %></td>
<td><%= user.email_address %></td>
<td><a href="/<%= user.id %>/show">Show</a>   <a href="/<%= user.id %>/edit">Edit</a>  <%= link_to 'destroy',"/#{user.id}", :method => :delete %></td>
  </tr>
  <% end %>
</table>
<a href="/new">New User</a>
